{
  "name": "vue-iotequ-admin",
  "version": "3.0.0",
  "description": "A magical vue admin with code generator and spring boot api server",
  "author": "Qinyoyo",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --fix --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "axios": "^0.19.0",
    "better-scroll": "^1.15.2",
    "clipboard": "^2.0.4",
    "codemirror": "^5.49.2",
    "core-js": "^3.3.3",
    "driver.js": "^0.9.8",
    "dropzone": "^5.5.1",
    "echarts": "^4.8.0",
    "element-ui": "^2.12.0",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.4.5",
    "js-cookie": "^2.2.1",
    "jsonlint": "^1.6.3",
    "jszip": "^3.2.2",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.1.0",
    "showdown": "^1.9.0",
    "sortablejs": "^1.10.1",
    "tui-editor": "^1.4.7",
    "vue": "^2.6.10",
    "vue-count-to": "^1.0.13",
    "vue-cropper": "^0.4.9",
    "vue-i18n": "^8.15.0",
    "vue-infinite-loading": "^2.4.4",
    "vue-router": "3.1.6",
    "vue-splitpane": "^1.0.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.7.0",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.6.2",
    "@nutui/babel-plugin-separate-import": "^1.2.0",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.7.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.2.2",
    "connect": "^3.7.0",
    "es6-promise": "^4.2.8",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "html-webpack-plugin": "^3.2.0",
