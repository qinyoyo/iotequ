    "lint-staged": "^9.4.2",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.13.0",
    "plop": "^2.5.2",
    "runjs": "^4.4.2",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "script-loader": "^0.7.2",
    "serve-static": "^1.14.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
